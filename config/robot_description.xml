<?xml version="1.0" encoding="utf-8"?>
<robot name="quadruped_robot">

<!--
	<lib_paths >
    <propagate path="/home/silence/Workspace/qr_ws/devel/lib/libpropagate_lib.so"/>
    <hw_unit path="/home/silence/Workspace/qr_ws/devel/lib/libhardware_lib.so"/>
  </lib_paths>
-->

	<propagates name="propagates" >
		<channel name="pcan" type="middleware::PcanChannel" />
	</propagates>
	
	<hardwares name="hardwares" list="joints" >
		<joints type="middleware::Joint" channel="pcan" name="joints" >
			<joint name="fl_yaw"  leg="fl" jnt="yaw"  />
			<joint name="fl_knee" leg="fl" jnt="knee" />
			<joint name="fl_hip"  leg="fl" jnt="hip"  />
			<joint name="hl_yaw"  leg="hl" jnt="yaw"  />
			<joint name="hl_knee" leg="hl" jnt="knee" />
			<joint name="hl_hip"  leg="hl" jnt="hip"  />
			<joint name="fr_yaw"  leg="fr" jnt="yaw"  />
			<joint name="fr_knee" leg="fr" jnt="knee" />
			<joint name="fr_hip"  leg="fr" jnt="hip"  />
			<joint name="hr_yaw"  leg="hr" jnt="yaw"  />
			<joint name="hr_knee" leg="hr" jnt="knee" />
			<joint name="hr_hip"  leg="hr" jnt="hip"  />
		</joints>

		<touchdowns type="middleware::TouchDown" channel="pcan" name="touchdowns">
			<touchdown name="fl_td" leg="fl" />
			<touchdown name="hl_td" leg="hl" />
			<touchdown name="fr_td" leg="fr" />
			<touchdown name="hr_td" leg="hr" />
		</touchdowns>

		<power type="middleware::Power" name="power" />
	</hardwares>

</robot>

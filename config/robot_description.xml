		<joint type="middleware::Joint" channel="pcan"
			name="fr_yaw"  leg="fr" jnt="yaw"  />

		<joint type="middleware::Joint" channel="pcan"
			name="fr_knee" leg="fr" jnt="knee" />

		<joint type="middleware::Joint" channel="pcan"
			name="fr_hip"  leg="fr" jnt="hip"  />

		<joint type="middleware::Joint" channel="pcan"
